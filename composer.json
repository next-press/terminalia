{
    "name": "next-press/terminalia",
    "description": "Beautiful command line output for Laravel.",
    "type": "library",
    "require-dev": {
        "tightenco/duster": "^2.0",
        "pestphp/pest": "^2.6",
        "mockery/mockery": "^1.5",
        "spatie/ray": "^1.37"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Terminalia\\": "src/",
            "Terminalia\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joe Tannenbaum",
            "email": "joe@joe.codes"
        }
    ],
    "require": {
        "spatie/fork": "^1.1",
        "illuminate/collections": "^11",
        "illuminate/console": "^11",
        "illuminate/contracts": "^11",
        "illuminate/support": "^11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Terminalia\\Providers\\TerminaliaServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
